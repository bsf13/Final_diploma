Задание 1
SELECT c.login,
       COUNT(o.track)
FROM "Couriers" AS c JOIN "Orders" AS o ON c.id = o.courierId
WHERE o.inDelivery = true;

Задание 2
SELECT track AS track_number,
CASE
WHEN finished = true THEN 2
WHEN cancelled = true THEN -1
WHEN "inDelivery" = true THEN 1
ELSE 0
END
AS order_status
FROM "Orders";
